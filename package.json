{
  "name": "extract-openapi-req",
  "version": "1.0.2",
  "description": "Extract OpenAPI metadata from HTTP requests",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./frameworks/hono": {
      "types": "./dist/frameworks/hono.d.ts",
      "import": "./dist/frameworks/hono.mjs",
      "require": "./dist/frameworks/hono.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest",
    "dev": "npm run build && cd examples/hono && npm run dev"
  },
  "keywords": [
    "openapi",
    "swagger",
    "request",
    "metadata",
    "extract",
    "path-matching",
    "hono",
    "middleware"
  ],
  "author": "ahmedrowaihi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmedrowaihi/extract-openapi-req.git"
  },
  "bugs": {
    "url": "https://github.com/ahmedrowaihi/extract-openapi-req/issues"
  },
  "homepage": "https://github.com/ahmedrowaihi/extract-openapi-req#readme",
  "dependencies": {
    "path-to-regexp": "^6.2.1"
  },
  "peerDependencies": {
    "hono": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "hono": "^4.7.8",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
